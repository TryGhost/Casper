{
  "extends": [
    "config:base",
    ":maintainLockFilesWeekly",
    "schedule:earlyMondays",
    ":automergeMinor"
  ],
  "travis": { "enabled": true },
  "node": {
    "supportPolicy": ["lts_latest"]
  },
  "packageRules": [
    {
      "packagePatterns": ["^postcss", "css", "autoprefixer"],
      "groupName": "css processors"
    },
    {
      "packagePatterns": ["gulp"],
      "groupName": "gulp"
    }
  ]
}
